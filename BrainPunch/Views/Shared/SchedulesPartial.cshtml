            @Html.DisplayNameFor(model => model.SecondOpponentID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ScheduleDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WinnerID)
        </th>
        @*<th></th>*@
    </tr>

@foreach (var item in Model) {
