@model BrainPunch.Models.Member

@{
    ViewBag.Title = "Account";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-md-12">
        <h3>Welcome, @string.Format("{0} {1}", Model.FirstName, Model.LastName)</h3>
    </div>
    <div class="col-md-12">
        <br />
        <h4>List of Schedules</h4>
        @Html.Partial("SchedulesPartial", (IEnumerable<BrainPunch.Models.Game_Schedule>)ViewBag.Schedules)
    </div>
</div>